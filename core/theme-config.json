  "components": {
    "waybar": {
      "height": 42,
      "margin_top": 8,
      "margin_sides": 16,
      "modules_left": ["custom/logo", "hyprland/workspaces", "hyprland/window"],
      "modules_center": ["clock"],
      "modules_right": [
        "custom/zerotier",
        "network",
        "bluetooth",
        "pulseaudio",
        "battery",
        "tray",
        "custom/config",
