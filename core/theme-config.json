{
  "meta": {
    "name": "Cyberpunk Medieval - Sharp",
    "version": "2.1",
    "author": "System",
    "description": "Sharp cyberpunk theme with medieval touches - computers in the dark ages"
  },
  "colors": {
    "primary": {
      "primary": "#0d0c0b",
      "secondary": "#1a1815",
      "tertiary": "#2c2a25",
      "quaternary": "#0d0c0b"
    },
    "accent": {
      "primary": "#a78a4f",
      "secondary": "#4e4e50",
      "tertiary": "#70563e",
      "quaternary": "#7a1c1c"
    },
    "neutral": {
      "primary": "#3a2f28",
      "secondary": "#121212",
      "tertiary": "#4c4c4e",
      "quaternary": "#2f2f2f"
    },
    "text": {
      "primary": "#e8e1cf",
      "secondary": "#b4a990",
      "accent": "#a78a4f",
      "muted": "#777166"
    },
    "status": {
      "success": "#7d945d",
      "warning": "#c29e53",
      "error": "#7a1c1c",
      "info": "#8f7f56"
    },
    "semantic": {
      "border_active": "#4c4c4e",
      "border_inactive": "#2f2f2f",
      "accent_primary": "#a78a4f",
      "accent_secondary": "#4e4e50",
      "shadow": "#000000"
    }
  },
  "typography": {
    "font_primary": "JetBrains Mono Nerd Font",
    "font_secondary": "Fira Code Nerd Font",
    "size_small": 11,
    "size_normal": 13,
    "size_large": 16,
    "size_title": 20,
    "icon_small": 16,
    "icon_medium": 24,
    "icon_large": 32
  },
  "spacing": {
    "gaps_inner": 10,
    "gaps_outer": 20,
    "border_width": 2,
    "rounding": 0,
    "margins": {
      "small": 2,
      "medium": 5,
      "large": 12,
      "xlarge": 20
    }
  },
  "effects": {
    "blur": {
      "enabled": true,
      "size": 6,
      "passes": 2,
      "vibrancy": 0.1
    },
    "shadow": {
      "enabled": true,
      "range": 15,
      "render_power": 2
    },
    "animations": {
      "enabled": false,
      "speed_multiplier": 1.2,
      "curves": {
        "primary": "0.25, 0.46, 0.45, 0.94",
        "secondary": "0.68, -0.55, 0.265, 1.55",
        "tertiary": "0.23, 1, 0.320, 1",
        "quaternary": "0.175, 0.885, 0.320, 1.275"
      }
    }
  },
  "workspaces": {
    "mode": "virtual_desktops",
